{% for name, attributeType in types %}
  {% set createRouteName = 'doop_sylius_product_variant_attribute_plugin_admin_'~metadata.name~'_create' %}

  <a href="{{ path(createRouteName, { 'type': name }) }}" id="{{ name }}" class="item">
    {% set label = 'sylius.form.attribute_type.' ~ attributeType.type %}
    {{ label|trans }}
  </a>
{% endfor %}
